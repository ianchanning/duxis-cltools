{
  "name": "@imec-apt/duxis-cltools",
  "version": "0.3.1",
  "description": "Duxis project command line development tools.",
  "bin": {
    "dx": "./dx.sh"
  },
  "scripts": {
    "install": "run-s install:check-node-version install:completion",
    "install:completion": "bash scripts/install-completion.sh",
    "install:check-node-version": "bash scripts/check-node-version.sh",
    "test": "mocha --bail --recursive tests",
    "test:watch": "nodemon --watch js --watch tests --ext js,jsx,json,yml --exec 'mocha --bail --recursive tests'"
  },
  "dependencies": {
    "commander": "2.11.0",
    "fs-extra": "4.0.2",
    "js-yaml": "3.10.0",
    "lodash": "4.17.4",
    "npm-check": "5.4.5"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "mocha": "4.0.1",
    "nodemon": "1.12.1",
    "npm-run-all": "4.1.1"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imec-apt/duxis-cltools"
  },
  "keywords": [
    "duxis",
    "apt",
    "imec",
    "cli"
  ],
  "author": "wouter.vandenbroeck@imec.be",
  "license": "UNLICENSED"
}
